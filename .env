NODE_ENV="development"
APP_NAME='backend'
APP_PORT=4002

# database
DB_HOST="localhost"
DB_PORT=5432
DB_USER="postgres"
DB_PASSWORD="root"
DB_DATABASE="nest_poc_db"

#Seeders
FILESYSTEM_DISK=local
SEED_FILES_PATH=seeds/

GCP_CREDENTIALS_PATH=src/infrastructure/storage/gcs-keys.json
GCP_BUCKET_NAME=BUCKET_NAME

#RabbitMq
RABBITMQ_DSN=amqp://localhost:5672/
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
FAILED_MESSAGE_IMMEDIATE_RETRIES=5
FAILED_MESSAGE_DELAYED_RETRIES=3
DISPATCH_MESSAGE_LIMIT=10
CONSUME_MESSAGE_LIMIT=10
RETRY_QUEUE_MESSAGE_TTL=10000
RABBITMQ_HEARTBEAT_INTERVAL=30
RABBITMQ_FANOUT_EXCHANGE=sagittarius-a-nest-poc
RABBITMQ_DIRECT_EXCHANGE=sagittarius-a-direct-nest-poc
RABBITMQ_ERROR_BINDING_KEY=hospital.dead-letter
#patient
RABBITMQ_PRIMARY_QUEUE_PATIENT=hospital.patient
RABBITMQ_RETRY_QUEUE_PATIENT=hospital.patient.events-retry
RABBITMQ_RETRY_BINDING_KEY_PATIENT=hospital.patient.events-retry
#doctor
RABBITMQ_PRIMARY_QUEUE_DOCTOR=hospital.doctor
RABBITMQ_RETRY_QUEUE_DOCTOR=hospital.doctor.events-retry
RABBITMQ_RETRY_BINDING_KEY_DOCTOR=hospital.doctor.events-retry

